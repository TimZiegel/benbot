{
  "name": "benbot",
  "version": "2.0.0",
  "description": "A discord bot that performs the important task of posting sloth pics. And some other stuff too, I guess.",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "test": "nodemon --exec babel-node src/index.js",
    "start": "node dist/index.js",
    "build": "rimraf dist && npx babel src --out-dir dist",
    "gcp-build": "npm run build",
    "gcp-config": "gcloud config set project carbide-legend-238019",
    "gcp-deploy": "gcloud app deploy",
    "deploy": "npm run gcp-config && npm run gcp-deploy",
    "publish": "npm run deploy"
  },
  "author": "timziegel",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "discord.js": "^11.4.2",
    "dotenv": "^7.0.0",
    "mad-libber": "^1.1.0"
  }
}
